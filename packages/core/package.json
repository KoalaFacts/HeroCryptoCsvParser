{
  "name": "@koalafacts/hero-crypto-csv-parser",
  "version": "0.2.3",
  "description": "Universal cryptocurrency transaction parser for exchange exports, tax reporting, and portfolio tracking. Supports multiple formats beyond CSV.",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./tax": {
      "import": "./dist/tax/index.js",
      "types": "./dist/tax/index.d.ts"
    }
  },
  "scripts": {
    "build": "npm run lint && npm run clean && tsc -p tsconfig.build.json",
    "dev": "tsc --watch -p tsconfig.build.json",
    "clean": "rimraf dist",
    "lint": "biome check . --diagnostic-level=error",
    "test": "npm run lint && npm run typecheck && vitest",
    "test:ui": "npm run typecheck && vitest --ui",
    "test:coverage": "npm run typecheck && vitest --coverage",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "cryptocurrency",
    "cryptocurrency transaction parser",
    "tax reporting",
    "portfolio tracking",
    "csv parser"
  ],
  "author": "KoalaFacts",
  "license": "MIT",
  "homepage": "https://github.com/KoalaFacts/hero-crypto-csv-parser#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KoalaFacts/hero-crypto-csv-parser.git",
    "directory": "packages/core"
  },
  "bugs": {
    "url": "https://github.com/KoalaFacts/hero-crypto-csv-parser/issues"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.2.5",
    "@types/node": "^24.7.0",
    "@vitest/coverage-v8": "^3.2.4",
    "rimraf": "^6.0.1",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ]
}
